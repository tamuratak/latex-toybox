{
	"comments": {
		"lineComment": "%"
	},
	"brackets": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["[", ")"],
		["(", "]"]
	],
	"autoClosingPairs": [
		["\\begin{", ""],
		["\\left(", ""],
		["\\left[", ""],
		["\\left{", ""],
		["\\bigl(", ""],
		["\\bigl[", ""],
		["\\bigl{", ""],
		["\\Bigl(", ""],
		["\\Bigl[", ""],
		["\\Bigl{", ""],
		["\\biggl(", ""],
		["\\biggl[", ""],
		["\\biggl{", ""],
		["\\Biggl(", ""],
		["\\Biggl[", ""],
		["\\Biggl{", ""],
		["\\(", ""],
		["\\[", ""],
		["\\{", ""],
		["@(", ""],
		["@[", ""],
		["@{", ""],
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["`", "'"]
	],
	"surroundingPairs": [
		["{", "}"],
		["[", "]"],
		["(", ")"],
		["\"", "\""],
		["'", "'"],
		["`", "'"],
		["$", "$"]
	],
	"indentationRules": {
		"increaseIndentPattern": "\\\\begin{(?!document)([^}]*)}(?!.*\\\\end{\\1})",
		"decreaseIndentPattern": "^\\s*\\\\end{(?!document)"
	},
	"folding": {
		"markers": {
			"start": "^\\s*%?\\s*(region|\\\\begingroup)\\b",
			"end": "^\\s*%?\\s*(endregion|\\\\endgroup)\\b"
		}
	},
	"autoCloseBefore": ";:.,={}])>\\` \n\t$",
	"wordPattern": {
		"pattern": "(__)|(\\*\\*)|(\\.\\.\\.)|(\\p{Alphabetic}|\\p{Number}|\\p{Nonspacing_Mark}){2,}",
		"flags": "u"
	}
}
